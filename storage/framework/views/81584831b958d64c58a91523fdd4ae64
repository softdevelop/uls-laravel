<script type='text/javascript'>
		function selectDriver(driverType)
		{
				var select = document.getElementById(driverType);
				var selectedVal = select.options[select.selectedIndex].value;
				$(".driverLinks").hide(); 
				$(".driverLink").hide();							
				if(selectedVal != "")
				{
	
						$("#" + driverType + "Links_" + selectedVal).show();
						$("#" + driverType + "Links").show();
				}
		}
</script>




