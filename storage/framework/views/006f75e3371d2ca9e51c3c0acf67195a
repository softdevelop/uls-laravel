<form id="evaluatorForm" method="post" action="/submitQuiz">
		<input type='hidden' id='evaluator_answers' name='evaluator_answers' value='' />
		<input type='hidden' id='evaluator_score' name='evaluator_score' value='' />
</form>
<script type='text/javascript'>
function calcQuiz()
{
    var score = 0;
    var answers = "";
    var numNone = 0;
    var evaluatorAnswers = document.getElementById("evaluator_answers");
    var evaluatorScore = document.getElementById("evaluator_score");

    for(i = 1; i <= 8; i++)
    {
        var questionScore = 0;
        var questionAnswers = "";
        var questionNone = 0;
        numCheckboxes = document.getElementById("tabInfo" + i).value;
        for(j = 1; j <= numCheckboxes; j++)
        {
            quizAnswer = isQuizChecked(i, j);
            if(quizAnswer[0] == 1)
            {
								if(i == 1)
                {
										updateQuizSelect(i, j);
								}
                if(i == 1 || i == 2 || i == 3 || i == 5 || i == 6)
                {
                    if(questionScore == 0) questionScore = 1;
                }
                else if(i == 4)
                {
                    if(questionScore < 3) questionScore++;
										
                }
            }
            if(quizAnswer[1] != "")
            {
                if(quizAnswer[0] == 0) questionNone++;
                if(questionAnswers != "") questionAnswers += "|";
                questionAnswers += quizAnswer[1];
            }
        }
        if(questionScore > 0) questionNone = 0;
        numNone = numNone + questionNone;
        question = document.getElementById("question_" + i).innerHTML;
        if(answers != "") answers += "`";
        answers += question + "~" + questionAnswers;
        score = score + questionScore;
        
    }    
    evaluatorAnswers.value = answers;
    evaluatorScore.value = score;		
		$("#general-contact-form_extra1").value = answers;
		$("#general-contact-form_extra2").value = score;
		$("#see-demo-form_extra1").value = answers;
		$("#see-demo-form_extra2").value = score;
		$("#test-material-form_extra1").value = answers;
		$("#test-material-form_extra2").value = score;
		$("#name-email-form_extra1").value = answers;
		$("#name-email-form_extra2").value = score;
	  $("#name-email-update-form_extra1").value = answers;
		$("#name-email-update-form_extra2").value = score;
		
		$("#evaluate-first-steps").hide();
    if(numNone < 2)
    {
				nextTab(8, 10, "");
    }
    else
    {
				nextTab(8, 9, "");
    }
}
function isQuizChecked(question, number)
{
    var answer = "";
    var score = 0;
    var checkbox = document.getElementById("q" + question + "_" + number);
    if(checkbox != null)
    {
        if($("#q" + question + "_" + number).is(':checked'))
        {
            answer = $("label[for='q" + question + "_" + number + "']").html();
            if($("#q" + question + "_" + number).attr("name") != "none")
            {
                score = 1;
            }
        }
    }
    return [score, answer];
}
function submitQuizEmail()
{
    var score = document.getElementById("evaluator_score").value;
    var pic = document.getElementById("evaluator-results-pic1");
    if(score <= 5)
    {
        $("#evaluate-results-pic1").show();
        $("#evaluate-results-pic2").hide();
        $("#evaluate-results-pic3").hide();
        $("#evaluate-results-pic4").hide();
    }
    else if(score == 6)
    {
        $("#evaluate-results-pic1").hide();
        $("#evaluate-results-pic2").show();
        $("#evaluate-results-pic3").hide();
        $("#evaluate-results-pic4").hide();
    }
    else if(score == 7)
    {
        $("#evaluate-results-pic1").hide();
        $("#evaluate-results-pic2").hide();
        $("#evaluate-results-pic3").show();
        $("#evaluate-results-pic4").hide();
    }
    else if(score >= 8)
    {
        $("#evaluate-results-pic1").hide();
        $("#evaluate-results-pic2").hide();
        $("#evaluate-results-pic3").hide();
        $("#evaluate-results-pic4").show();
    }
    
    nextTab(10, 11, "");
}
function updateQuizSelect(question, number)
{
		var optionValue = $("label[for='q" + question + "_" + number + "']").html();
    $("#general-contact-form_market option[value='" + optionValue + "']").prop('selected', true);
    $("#see-demo-form_market option[value='" + optionValue + "']").prop('selected', true);
    $("#test-material-form_market option[value='" + optionValue + "']").prop('selected', true);
}
</script>

