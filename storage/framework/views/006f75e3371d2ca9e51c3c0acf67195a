<script type='text/javascript'>
	function see_demo_form_marketSwitch_<?php echo  $randNum;?>(select)
	{
		var value = select.options[select.selectedIndex].value;
		if(value == "Other")
		{
			$("#see-demo-form_otherDiv_<?php echo  $randNum;?>").show();
		}
		else
		{
			$("#see-demo-form_otherDiv_<?php echo  $randNum;?>").hide();
		}
	}
</script><?php /* End See Demonstration Form Block */ ?>
