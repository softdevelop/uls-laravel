<script type="text/javascript">
  function nextTab(curTab, nextTab, numCheckboxes)
  {
  		var allow = true;
  		if(numCheckboxes != "" && numCheckboxes != 0)
        {
          	var foundCheck = false;
        	for(i = 1; i <= numCheckboxes; i++)
            {
            		if(!foundCheck)
                    {
                     		check = document.getElementById("q" + curTab + "_" + i);
                      		if(check != null)
                            {
                             		if(check.checked) foundCheck = true; 
                            }
                    }
            }
            if(!foundCheck) allow = false;
        }
    	if(allow)
        {
            $("#tab" + curTab).hide();
            $("#tab" + nextTab).show();
        }
    	else
        {
         		alert("Please select an option before continuing.  Thank you!"); 
        }
  }
  function prevTab(curTab,prevTab)
  {
  		$("#tab" + curTab).hide();
  		$("#tab" + prevTab).show();
  }
  
</script>

<?php /* End Large Numbered List Block */ ?>
