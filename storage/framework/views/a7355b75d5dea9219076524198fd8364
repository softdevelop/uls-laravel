	function general_contact_form_marketSwitch_<?php echo  $randNum;?>(select)
	{
		var value = select.options[select.selectedIndex].value;
		if(value == "Other")
		{
			$("#general-contact-form_otherDiv_<?php echo  $randNum;?>").show();
		}
		else
		{
			$("#general-contact-form_otherDiv_<?php echo  $randNum;?>").hide();
		}
	}
